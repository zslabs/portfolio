{
  "name": "zslabs",
  "private": true,
  "description": "Portfolio site for software engineer, Zach Schnackel",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "version": "2.5.0",
  "author": "Zach Schnackel <info@zslabs.com>",
  "dependencies": {
    "@mdx-js/mdx": "1.6.18",
    "@mdx-js/react": "1.6.18",
    "@pauliescanlon/gatsby-mdx-embed": "0.0.29",
    "@wzuo/babel-plugin-polished": "1.2.0",
    "babel-eslint": "10.1.0",
    "chaoskit": "https://github.com/gremlin/chaoskit/tarball/b048a9215d5c1157f356a394bf971eb280282a5c/chaoskit.tar.gz",
    "create-emotion-server": "10.0.27",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-loader": "3.0.3",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react-hooks": "4.1.2",
    "gatsby": "2.24.64",
    "gatsby-plugin-canonical-urls": "2.3.11",
    "gatsby-plugin-emotion": "4.3.11",
    "gatsby-plugin-feed": "2.5.12",
    "gatsby-plugin-google-analytics": "2.3.14",
    "gatsby-plugin-manifest": "2.4.31",
    "gatsby-plugin-mdx": "1.2.40",
    "gatsby-plugin-mdx-frontmatter": "0.0.2",
    "gatsby-plugin-netlify": "2.3.15",
    "gatsby-plugin-react-helmet": "3.3.11",
    "gatsby-plugin-sharp": "2.6.36",
    "gatsby-plugin-sitemap": "2.4.13",
    "gatsby-remark-copy-linked-files": "2.3.16",
    "gatsby-remark-images": "3.3.30",
    "gatsby-remark-relative-source": "0.0.4",
    "gatsby-remark-smartypants": "2.3.11",
    "gatsby-source-filesystem": "2.3.30",
    "gatsby-transformer-sharp": "2.5.15",
    "github-slugger": "1.3.0",
    "mdx-utils": "0.2.0",
    "netlify-plugin-gatsby-cache": "0.3.2",
    "prism-react-renderer": "1.1.1",
    "react": "16.13.1",
    "react-copy-to-clipboard": "5.0.2",
    "react-dom": "16.13.1",
    "react-helmet": "6.1.0",
    "remark": "12.0.1",
    "root-units": "2.3.0",
    "zustand": "3.1.3"
  },
  "license": "MIT",
  "scripts": {
    "icons": "node ./utils/build-icons.js",
    "startup": "yarn && yarn run icons",
    "build": "yarn startup && GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build",
    "build-serve": "yarn build && gatsby serve",
    "develop": "yarn startup && gatsby develop --open",
    "lint": "eslint ./src ./utils",
    "lint:fix": "eslint ./src ./utils --fix"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zslabs/zslabs"
  }
